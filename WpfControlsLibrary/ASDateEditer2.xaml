<UserControl x:Class="WpfControlsLibrary.ASDateEditer2"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfControlsLibrary"
             mc:Ignorable="d" 
             >
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="auto"/>
        </Grid.ColumnDefinitions>

        <TextBox x:Name="asdeTextBox" IsReadOnly="False" VerticalContentAlignment="Center"/>
        <Button x:Name="asdeButton" Grid.Column="1" Content="..."/>
        <DatePicker x:Name="asdeDatePicker" Grid.Column="1" Width="0" IsTabStop="False" IsHitTestVisible="False" />
        
    </Grid>
</UserControl>
