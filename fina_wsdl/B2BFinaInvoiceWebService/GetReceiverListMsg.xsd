<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns="http://fina.hr/eracun/b2b/sync/GetReceiverList/v0.1" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:iwsc="http://fina.hr/eracun/b2b/invoicewebservicecomponents/v0.1" targetNamespace="http://fina.hr/eracun/b2b/sync/GetReceiverList/v0.1" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xsd:import namespace="http://fina.hr/eracun/b2b/invoicewebservicecomponents/v0.1" schemaLocation="InvoiceWebServiceComponents.xsd"/>
	<xsd:element name="GetReceiverListMsg">
		<xsd:annotation>
			<xsd:documentation>Poruka za dohvat liste primatelja iz registra.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="iwsc:HeaderSupplier">
					<xsd:annotation>
						<xsd:documentation>Zaglavlje poruke</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Data">
					<xsd:annotation>
						<xsd:documentation>XML s podacima</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="ReceiverList">
								<xsd:annotation>
									<xsd:documentation>Omotač upita za dohvat liste ulaznih računa</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element ref="iwsc:Filter">
											<xsd:annotation>
												<xsd:documentation>Filter za dohvat.</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="GetReceiverListAckMsg">
		<xsd:annotation>
			<xsd:documentation>Sinkroni odgovor na poruku za dohvat liste primatelja iz registra.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="iwsc:MessageAck"/>
				<xsd:element name="BusinessBranchList" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Omotač upita za dohvat liste primatelja iz registra</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="BusinessBranch" minOccurs="0" maxOccurs="unbounded">
								<xsd:annotation>
									<xsd:documentation>Poslovna jedinica</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="BuyerID" type="xsd:string">
											<xsd:annotation>
												<xsd:documentation>Identifikator poslovne jedinice (BuyerID).</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="SchemeName" type="xsd:string">
											<xsd:annotation>
												<xsd:documentation>Vrsta sheme poslovne jedinice (GLN, JUS, RKP,...).</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="ReceiptType" type="xsd:string" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Tip zaprimanja (1-UBL, 2-UBL EN, 3-CII)</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="BusinessBranchCode" type="xsd:string">
											<xsd:annotation>
												<xsd:documentation>Šifra poslovne jedinice.</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="BusinessBranchName" type="xsd:string">
											<xsd:annotation>
												<xsd:documentation>Naziv poslovne jedinice.</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element ref="iwsc:PostalAddress">
											<xsd:annotation>
												<xsd:documentation>Adresa</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="Activity" type="xsd:integer">
											<xsd:annotation>
												<xsd:documentation>Naziv poslovne jedinice.</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="Receiver" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Naziv tvrtke primatelja računa.</xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="RegistrationName" type="xsd:string">
														<xsd:annotation>
															<xsd:documentation>Naziv tvrtke primatelja računa.</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="ISOCode" type="xsd:string" minOccurs="0">
														<xsd:annotation>
															<xsd:documentation>ISO kod države (HR)</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="CompanyID" type="xsd:string">
														<xsd:annotation>
															<xsd:documentation>OIB</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element ref="iwsc:PostalAddress">
														<xsd:annotation>
															<xsd:documentation>Adresa</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
