MessageBox("...Testing find_index...");

DEFINE abc type a size 10 array 100;

abc[0] = "AAA";
abc[1] = "BBB";
abc[2] = "CCC";
abc[3] = "DDD";
abc[4] = "EEE";

MessageBox(find_index(abc, "DDD")); //Output: 3 (Found)
MessageBox(find_index(abc, "AAA")); //Output: 0 (Found)
MessageBox(find_index(abc, "FFF")); //Output: -1 (Not found)

quit;
