MessageBox("...Testing namespace 2... (Example by Copilot AI)");

namespace(MyMath) {
    function square(x) {
        return x * x;
    }

    function cube(x) {
        return x * x * x;
    }
}

namespace(MyText) {
    function shout(text) {
        return text.ToUpper();
    }
}

// Accessing functions from namespaces
MyMath.square(5);   // Returns 25
MyText.shout("hello"); // Returns "HELLO"


quit;