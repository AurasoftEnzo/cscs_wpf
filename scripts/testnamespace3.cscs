MessageBox("...Testing namespace 3... (Example by GROK AI)");

function example() {
    namespace MyNamespace {
        x = 10; // Variable x is local to MyNamespace
        MessageBox("Inside MyNamespace: x = " + x);
    }
    MessageBox("Outside MyNamespace: x = " + x); // x outside the namespace is unaffected
}

x = 100; // Global variable x
example();

quit;