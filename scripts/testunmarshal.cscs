MessageBox("...Testing unmarshal...");

// Define an object
person = {};
person["name"] = "Alice";
person["age"] = 30;

// Marshal the object to a string
marshalled = Marshal(person);
MessageBox("Marshalled string: " + marshalled);

// Unmarshal it back to an object
unmarshalled = Unmarshal(marshalled);
MessageBox("Name: " + unmarshalled["name"]);
MessageBox("Age: " + unmarshalled["age"]);

quit;