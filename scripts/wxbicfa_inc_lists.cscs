function ceKonto@clicked()
{
    F2ListQueryString = "SELECT distinct gk_ac_konto, gk_ac_opis, CASE when kdib_ln_dp= 'D' THEN 'Odliv' ELSE 'Priliv' END as Trans FROM " + trenutnaGodinaBaza + ".dbo.kpkdibln left join " + trenutnaGodinaBaza + ".dbo.nkgkacct ON kdib_ln_konto = gk_ac_konto where gk_ac_opis like '%{var2}%' AND gk_ac_konto like '{var1}%'";
    //if(searchSklSifra != 0){
    //    F2ListQueryString += " and PKMK_SKL_CODE = '{var1}'";
    //}
    F2ListQueryString += " ORDER BY gk_ac_konto";
    
    Konto = "";
    KontoNaziv = "";
    sifraFieldPointer -> Konto;
    NazivFieldPointer -> KontoNaziv; 
    

    windowTitle = "F2 lista konta";
    headersArray = {"Konto", "Naziv konta", "Priliv/odliv"};
    
    ShowF2List();
    
    if(&sifraFieldPointer != ""){
        Konto = &sifraFieldPointer;
        if (Konto != "")
        {
            Konto = strtrim(Konto);
            KONTO_str = (KONTO_str == "") ? (KONTO_str + Konto) : (KONTO_str + "," + Konto);
        }    
    }
    searchSifra = "";
    searchNaziv = "";
}
