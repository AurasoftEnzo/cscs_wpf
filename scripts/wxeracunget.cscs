                sql_insert += "@PCode, @PDesc";
                sql_insert += ");";
                
                sqlParams = {};
                sqlParams.Add({"@recordId", NKPR_GL_BRPON});
                sqlParams.Add({"@brOpi", brOpi});
                sqlParams.Add({"@brOpi1", brOpi1});
                numOfLines++;
                sqlParams.Add({"@brLn", numOfLines});
                sqlParams.Add({"@MaUs", "O"});
                sqlParams.Add({"@PCode", "\\"});

                sqlParams.Add({"@PDesc", "Bilješka"}); ///// 50
                    
                result = SqlNonQuery(sql_insert, sqlParams);
                if (result == 0) {
                    throw "Greška prilikom unosa linije s referencom na note.";
                }

                noteNum += 2;
            }
        }
        
        return true;
        
    } catch (error) {
        throw "Greška prilikom unosa stavki: " + error;
    }
}

