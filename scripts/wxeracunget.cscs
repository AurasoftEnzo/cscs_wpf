                if (result == 0) {
                    throw "Greška prilikom unosa linije s referencom na attachment.";
                }
            }
        }
        
        //linije s referencama na note-ove
        if(Size(restOfNotes) > 0) {
            noteNum = 0;
            while( true ) {
                if(noteNum >= Size(restOfNotes)) {
                    break;
                }

                brOpi = restOfNotes[noteNum];
                brOpi1 = 0;
                if(noteNum + 1 < Size(restOfNotes)) {
                    brOpi1 = restOfNotes[noteNum + 1];
                }

                sql_insert = "INSERT INTO " + databaseName + ".dbo.NKNAERLN (";
                sql_insert += "NKPR_LN_INVNM, NKPR_LN_BROPI, NKPR_LN_BROPI1, NKPR_LN_BR_LN, NKPR_LN_MA_US, NKPR_LN_PCODE, ";
                sql_insert += "NKPR_LN_PDESC) VALUES (";
